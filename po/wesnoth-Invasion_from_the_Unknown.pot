#. [side]: type=Death Knight, id=Tanstafaal
#: ./Invasion_from_the_Unknown/scenarios/15_Shadows_of_Time.cfg:90
msgid "Tanstafaal"
msgstr ""

#. [side]: type=Orcish Slurbow, id=Tan Zalfargat
#: ./Invasion_from_the_Unknown/scenarios/15_Shadows_of_Time.cfg:122
msgid "Tan Zalfargat"
msgstr ""

#. [side]: type=Ancient Lich, id=Mal Hekuba
#. [unit]: type=Ancient Lich, id=Mal Hekuba
#: ./Invasion_from_the_Unknown/scenarios/15_Shadows_of_Time.cfg:149 ./Invasion_from_the_Unknown/scenarios/18_Armageddon.cfg:191
msgid "Mal Hekuba"
msgstr ""

